index="1" value="75"/><column index="2" value="65"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="sqlite_sequence" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="57"/><column index="2" value="52"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="Create Tables">CREATE TABLE Restaurants (
	restaurant_id	INTEGER NOT NULL,
	name			TEXT NOT NULL,
	description		TEXT,
	open_time		TEXT NOT NULL,
	close_time		TEXT NOT NULL,
	address			TEXT NOT NULL,
	city			TEXT NOT NULL,
	state			TEXT NOT NULL,
	zip_code		INTEGER NOT NULL,
	phone_number	TEXT,
	email_address	TEXT,
	CHECK(length(zip_code) &lt;= 5 AND
		zip_code &gt;= 0),
	PRIMARY KEY(restaurant_id AUTOINCREMENT)
);
CREATE TABLE Employees (
	employee_id		INTEGER NOT NULL,
	personnel_id		INTEGER NOT NULL,
	restaurant_id	INTEGER NOT NULL,
	role			TEXT NOT NULL,
	PRIMARY KEY(employee_id AUTOINCREMENT),
	FOREIGN KEY(restaurant_id) REFERENCES Restaurants(restaurant_id),
	FOREIGN KEY(personnel_id) REFERENCES Personnel(personnel_id)
);

CREATE TABLE Personnel (
	personnel_id	INTEGER NOT NULL,
	last_name	TEXT NOT NULL,
	first_name	TEXT NOT NULL,
	birthdate	TEXT NOT NULL,
	ssn			INTEGER UNIQUE NOT NULL,
	CHECK(length(ssn) &lt;= 9 AND
		ssn &gt;= 0),
	PRIMARY KEY(personnel_id AUTOINCREMENT)
);

CREATE TABLE Restaurant_Tags (
	tag_id	INTEGER NOT NULL,
	name	TEXT NOT NULL,
	PRIMARY KEY(tag_id AUTOINCREMENT)
);

CREATE TABLE Tags_To_Restaurants (
	restaurant_id	INTEGER NOT NULL,
	tag_id			INTEGER NOT NULL,
	PRIMARY KEY(restaurant_id, tag_id)
	FOREIGN KEY(restaurant_id) REFERENCES Restaurants(restaurant_id),
	FOREIGN KEY(tag_id) REFERENCES Restaurant_Tags(tag_id)
);

CREATE TABLE Menus (
	menu_id			INTEGER NOT NULL,
	restaurant_id	INTEGER NOT NULL,
	menu_name		TEXT NOT NULL,
	start_time		TEXT,
	end_time		TEXT,
	PRIMARY KEY(menu_id AUTOINCREMENT),
	FOREIGN KEY(restaurant_id) REFERENCES Restaurants(restaurant_id)
);

CREATE TABLE Menu_Items (
	item_id			INTEGER NOT NULL,
	menu_id			INTEGER NOT NULL,
	item_name		TEXT NOT NULL,
	price			INTEGER NOT NULL,
	description		TEXT,
	is_in_stock		INTEGER DEFAULT 0,
	CHECK(price &gt;= 0),
	PRIMARY KEY(item_id AUTOINCREMENT),
	FOREIGN KEY(menu_id) REFERENCES Menus(menu_id)
);

CREATE TABLE Item_Tags (
	tag_id		INTEGER NOT NULL,
	name		TEXT NOT NULL,
	PRIMARY KEY(tag_id AUTOINCREMENT)
);

CREATE TABLE Tags_To_Items (
	item_id		INTEGER NOT NULL,
	tag_id		INTEGER NOT NULL,
	PRIMARY KEY(item_id, tag_id),
	FOREIGN KEY(item_id) REFERENCES Menu_Items(item_id),
	FOREIGN KEY(tag_id) REFERENCES Item_Tags(tag_id)
);</sql><sql name="Clear Tables">DROP TABLE Employees;
