	VALUES (1, &quot;Milk Teas&quot;)</sql><sql name="Query Examples">-- Query Employees by name from Boba Bar
SELECT Personnel.personnel_id, Personnel.first_name, Personnel.last_name
	FROM Personnel, Employees, Restaurants
	WHERE Restaurants.name = &quot;Foo(d) &amp; Bar&quot;
	AND Employees.restaurant_id = Restaurants.restaurant_id
	AND Employees.personnel_id = Personnel.personnel_id
	GROUP BY Personnel.personnel_id;

-- Query Sandwich Restaurants
SELECT Restaurants.restaurant_id, Restaurants.name
	FROM Restaurants, Restaurant_Tags, Tags_To_Restaurants
	WHERE Restaurant_Tags.name = &quot;Sandwiches&quot;
	AND Tags_To_Restaurants.tag_id = Restaurant_Tags.tag_id
	AND Tags_To_Restaurants.restaurant_id = Restaurants.restaurant_id
	GROUP BY Restaurants.restaurant_id;

-- Query Locations of Restaurants with Tea
SELECT Restaurants.restaurant_id, Restaurants.name, Restaurants.address, Restaurants.city, Restaurants.state, Restaurants.zip_code
	FROM Restaurants, Restaurant_Tags, Tags_To_Restaurants
	WHERE Restaurant_Tags.name = &quot;Tea&quot;
	AND Tags_To_Restaurants.tag_id = Restaurant_Tags.tag_id
	AND Tags_To_Restaurants.restaurant_id = Restaurants.restaurant_id
	GROUP BY Restaurants.restaurant_id;

-- Query Restaurants with Vegan options
SELECT Restaurants.restaurant_id, Restaurants.name
	FROM Restaurants, Menus, Menu_Items, Tags_To_Items, Item_Tags
	WHERE Item_Tags.name = &quot;Vegan&quot;
	AND Tags_To_Items.tag_id = Item_Tags.tag_id
	AND Tags_To_Items.item_id = Menu_Items.item_id
	AND Menu_Items.menu_id = Menus.menu_id
	AND Menus.restaurant_id = Restaurants.restaurant_id
	GROUP BY Restaurants.restaurant_id;

-- Query all options from Foo(d) &amp; Bar
SELECT Menus.menu_id, Menus.menu_name, Menu_Items.item_id, Menu_Items.item_name, Menu_Items.price
	FROM Menu_Items, Item_Tags, Tags_To_Items, Restaurants, Menus
	WHERE Menu_Items.menu_id = Menus.menu_id
	AND Menus.restaurant_id = Restaurants.restaurant_id
	AND Restaurants.name = &quot;Foo(d) &amp; Bar&quot;
	GROUP BY Menu_Items.item_id
	ORDER BY Menus.menu_id;

-- Query Vegan options from Foo(d) &amp; Bar available at 10:13
SELECT Menu_Items.item_id, Menu_Items.item_name, Menu_Items.price
	FROM Menu_Items, Item_Tags, Tags_To_Items, Restaurants, Menus
	WHERE Item_Tags.name = &quot;Vegan&quot;
	AND Tags_To_Items.tag_id = Item_Tags.tag_id
	AND Tags_To_Items.item_id = Menu_Items.item_id
	AND Menu_Items.menu_id = Menus.menu_id
	AND Menus.restaurant_id = Restaurants.restaurant_id
	AND Restaurants.name = &quot;Foo(d) &amp; Bar&quot;
	AND &quot;10:13:00&quot; BETWEEN Menus.start_time AND Menus.end_time
	GROUP BY Menu_Items.item_id;

-- Query options from Foo(d) &amp; Bar's Drinks menu
SELECT Menu_Items.item_id, Menu_Items.item_name, Menu_Items.price
	FROM Menu_Items, Restaurants, Menus
	WHERE Menus.menu_name = &quot;Drinks&quot;
	AND Menu_Items.menu_id = Menus.menu_id
	AND Menus.restaurant_id = Restaurants.restaurant_id
	AND Restaurants.name = &quot;Foo(d) &amp; Bar&quot;
	GROUP BY Menu_Items.item_id;

-- Query non-alcoholic options from Foo(d) &amp; Bar's Drinks menu
SELECT Menu_Items.item_id, Menu_Items.item_name, Menu_Items.price
	FROM Menu_Items, Tags_To_Items, Item_Tags, Restaurants, Menus
	WHERE Menus.menu_name = &quot;Drinks&quot;
	AND Menu_Items.menu_id = Menus.menu_id
	AND Menus.restaurant_id = Restaurants.restaurant_id
	AND Restaurants.name = &quot;Foo(d) &amp; Bar&quot;
	GROUP BY Menu_Items.item_id
	EXCEPT 
	SELECT Menu_Items.item_id, Menu_Items.item_name, Menu_Items.price
		FROM Menu_Items, Tags_To_Items, Item_Tags, Restaurants, Menus
		WHERE Item_Tags.name = &quot;Alcohol&quot;
		AND Tags_To_Items.tag_id = Item_Tags.tag_id
		AND Tags_To_Items.item_id = Menu_Items.item_id;</sql><current_tab id="2"/></tab_sql></sqlb_project>
